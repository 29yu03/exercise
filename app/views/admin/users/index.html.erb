<div class="container", style="background-color: #EFEFEF;">
<h1>All Users</h1>

<table class="table">
  <thead>
    <tr>
      <th>Email</th>
      <th>名前</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.last_name %><%= user.first_name %></td>
        <td>
          <%= link_to '編集', edit_admin_user_path(user) %>
          　<%= link_to '削除（退会）', admin_user_path(user), method: :delete, data: { confirm: '本当に削除しますか？' } %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
